<%
include("/jagg/jagg.jag");
var site = require("/site/conf/site.json");
var log = new Log();
var encoder = Packages.org.wso2.carbon.ui.util.CharacterEncoder;
var accountId = encoder.getSafeText(request.getParameter('accountId'));
var invoiceId = encoder.getSafeText(request.getParameter('invoiceId'));
(function () {
    var user = jagg.getUser();
    if (user == null) {
        response.sendRedirect("../pages/index.jag");
        return;
    }
    var multiTenantUtils = Packages.org.wso2.carbon.utils.multitenancy.MultitenantUtils;
    var tenantDomain = multiTenantUtils.getTenantDomain(user);
    if (tenantDomain != null && !jagg.module("util").isUserInAdminRole()) {
        response.sendRedirect("../pages/index.jag");
        return;
    }
    var isMonetizationEnabledObj = jagg.module("monetizing").isMonetizationEnabled(tenantDomain);
    if (isMonetizationEnabledObj == null || isMonetizationEnabledObj.error || !isMonetizationEnabledObj.monetizationEnabled) {
        response.sendRedirect("../pages/index.jag");
        return;
    }

    jagg.render({
        "name": "page/base-new",
        "inputs": {
            "title": "WSO2 Cloud Mgt",
            "pagePath": "/site/pages/monetization-invoice.jag",
            "body": [
                {
                    "name": "layout/base",
                    "inputs": {
                        "title": "invoice",
                        "middle": [
                            {
                                "name": "monetizing/account/invoice",
                                "inputs":{
                                    "accountId": accountId,
                                    "invoiceId": invoiceId
                                }
                            }
                        ],
                        "breadcrumb": [
                            {
                                "name": "breadcrumb",
                                "inputs": {
                                    "pageName": "Subscriptions / Invoice"
                                }
                            }
                        ],
                        "tablinks": [
                            {
                                "name": "tablinks"
                            }
                        ]
                    }
                }
            ]
        }
    });
}());
%>